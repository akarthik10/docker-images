{
  extends: [
    'config:recommended',
  ],
  packageRules: [
    {
      matchUpdateTypes: [
        'minor',
        'patch',
        'pin',
      ],
      matchCurrentVersion: '!/^0/',
      automerge: true,
    },
    {
      matchPackageNames: [
        'n8nio/n8n',
        'yfinance',
      ],
      matchUpdateTypes: [
        'minor',
        'patch',
      ],
      automerge: true,
    },
  ],
  customManagers: [
    {
      customType: 'regex',
      managerFilePatterns: [
        '/\\.imagetag$/',
      ],
      matchStrings: [
        '#\\s*renovate:\\s*(?<depName>[^\\s]+)\\s*\\n[^=]+=\"(?<currentValue>[^\\s]+)\"',
      ],
      datasourceTemplate: 'docker',
    },
  ],
}
